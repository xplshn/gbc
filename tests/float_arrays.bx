extrn printf;

main() {
   float float_array[3];
   float32 float32_array[3];

   float_array[0], float32_array[0] = 1.1, 1.25;
   float_array[1], float32_array[1] = 2.2, 2.75;
   float_array[2], float32_array[2] = 3.3, 3.125;

   printf("float[0]: %g, float32[0]: %g\n", float_array[0], float32_array[0]);
   printf("float[1]: %g, float32[1]: %g\n", float_array[1], float32_array[1]);
   printf("float[2]: %g, float32[2]: %g\n", float_array[2], float32_array[2]);

   return(0);
}
