extrn printf;

int main() {
   float32 x, y, z;
   
   // Try accessing them before any assignment to see uninitialized values
   printf("Initial: x = %.3f, y = %.3f, z = %.3f\n", x, y, z);
   
   // Assign values
   x = 1.25;
   y = 2.75;
   z = x + y;  // This should be 4.0
   
   printf("After assignment: x = %.3f, y = %.3f, z = %.3f\n", x, y, z);
   
   // Try some arithmetic
   x = x * 2.0;  // Should be 2.5
   y = y / 2.0;  // Should be 1.375
   
   printf("After arithmetic: x = %.3f, y = %.3f, z = %.3f\n", x, y, z);
   
   return (0);
}